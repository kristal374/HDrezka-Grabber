{
  "name": "HDrezka Grabber",
  "description": "__MSG_manifest_description__",
  "version": "0.1",
  "default_locale": "en",
  "manifest_version": 2,
  "icons": {
    "48": "img/icons/48.png",
    "32": "img/icons/32.png",
    "16": "img/icons/16.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "mail.aleks.for.work@gmail.com",
      "strict_min_version": "102.0"
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["src/js/background.js"]
  },
  "permissions": [
    "scripting",
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "downloads",
    "<all_urls>"
  ],
  "web_accessible_resources": ["src/js/*"],
  "author": "mail.aleks.for.work@gmail.com",
  "homepage_url": "https://github.com/kristal374/HDrezka-Grabber"
}

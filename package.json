{
  "name": "hdrezka-grabber",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:popup": "vite -c vite.config.ts build",
    "build:background": "rollup -c rollup.config.js",
    "build": "concurrently \"npm run build:popup\" \"npm run build:background\"",
    "build:chromium": "node run-build.js --browser chromium",
    "build:firefox": "node run-build.js --browser firefox",
    "build:dev": "node run-build.js --browser all --production false --silent false",
    "build:prod": "node run-build.js --browser all --production true --zip true --keep-dist-dir false",
    "format": "prettier --write --cache ."
  },
  "dependencies": {
    "@base-ui/react": "1.1.0",
    "@datastructures-js/priority-queue": "^6.3.5",
    "@number-flow/react": "0.5.11",

    "@radix-ui/react-popover": "^1.1.1",
    "@reduxjs/toolkit": "^2.2.5",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "fast-deep-equal": "^3.1.3",
    "idb": "^8.0.3",
    "lucide-react": "0.544.0",
    "mediabunny": "^1.29.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "6.0.0",
    "react-icons": "^5.5.0",
    "react-mentions": "^4.4.10",
    "react-redux": "^9.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "3.3.1",
    "use-storage-state": "^5.0.0"

    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "clsx": "2.1.1",
    "fast-deep-equal": "^3.1.3",
    "idb": "^8.0.3",
    "lucide-react": "0.544.0",
    "mediabunny": "1.29.1",
    "pako": "^2.1.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "6.0.0",
    "react-inspector": "^9.0.0",
    "react-mentions": "^4.4.10",
    "react-redux": "^9.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "3.4.0"
